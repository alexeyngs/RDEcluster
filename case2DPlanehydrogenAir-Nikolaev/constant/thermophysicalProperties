thermoType
{
    type            heRDEThermo;
    mixture         RDEMixtureNikolaev;
    transport       const;
    thermo          RDEThermo;
    equationOfState perfectGas;
    specie          RDEspecie;
    energy          absoluteEnthalpy;
}

species
(
    H2
    O2
    N2
    Ar
    H2O
);
