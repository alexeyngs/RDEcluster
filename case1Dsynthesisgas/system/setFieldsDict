FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volVectorFieldValue U (0.0 0.0 0.0)
    volScalarFieldValue T 300.0
    volScalarFieldValue p 101325.0
);

regions
(
    boxToCell
    {
        box (0.0 0.0 0.0) (0.2 0.1 0.1);
        fieldValues
        (
            volScalarFieldValue T 3500.0
            volScalarFieldValue p 4053000.0
        );
    }
);


